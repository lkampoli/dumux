[Grid]
UpperRight = 1 1
Cells = 4 4

[Problem]
Name = test_ff_analytic_rans
EnableGravity = false
PrintL2Error = true
EnableInertiaTerms = true

[Component]
LiquidDensity  = 1.0 # kg/m^3
LiquidKinematicViscosity = 1.0 # m^2/s
